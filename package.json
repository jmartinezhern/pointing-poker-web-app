{
  "name": "pointing-poker-web-app",
  "repository": "github.com/jmartinezhern/pointing-poker",
  "version": "0.0.1",
  "description": "Web Application for pointingpoker.app",
  "author": "Jorge Martinez Hernandez",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://pointingpoker.app",
  "scripts": {
    "clean": "rm -rf ./dist ./.cache ./node_modules ./src/graphql/generated-types.ts",
    "codegen": "graphql-codegen -r dotenv/config --config codegen.yml",
    "commit": "git-cz",
    "tsc:check": "tsc --noEmit -p .",
    "eslint:check": "eslint 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "test": "jest --runInBand",
    "test:coverage": "npm run test -- --coverage",
    "build": "parcel build --no-cache src/index.html",
    "test:watch": "jest --watch",
    "storybook:start": "start-storybook",
    "storybook:build": "build-storybook -c .storybook -o dist-storybook"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "moduleNameMapper": {
      "^~(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": {
    "production": [
      ">1% in US",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.53",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-context": "1.0.20",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "apollo-link-ws": "1.0.20",
    "graphql": "15.0.0",
    "react": "16.13.1",
    "react-apollo": "3.1.5",
    "react-apollo-hooks": "0.5.0",
    "react-dom": "16.13.1",
    "react-google-charts": "3.0.15",
    "react-router-dom": "5.2.0",
    "subscriptions-transport-ws": "0.9.16",
    "uuid": "8.0.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.4",
    "@graphql-codegen/cli": "1.15.0",
    "@graphql-codegen/introspection": "1.13.5",
    "@graphql-codegen/typescript": "1.13.5",
    "@graphql-codegen/typescript-operations": "1.13.5",
    "@graphql-codegen/typescript-react-apollo": "1.17.0",
    "@graphql-codegen/typescript-resolvers": "1.13.5",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addon-notes": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/cli": "5.3.19",
    "@storybook/react": "5.3.19",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/graphql": "14.5.0",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.5",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/storybook__react": "5.2.1",
    "@types/uuid": "7.0.3",
    "@types/ws": "7.2.4",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.33.0",
    "babel-loader": "8.1.0",
    "babel-types": "6.26.0",
    "codecov": "3.7.1",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "jest": "26.0.1",
    "less": "3.11.1",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.5",
    "ts-jest": "26.0.0",
    "ts-loader": "8.0.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.9.6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
