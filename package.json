{
  "name": "pointing-poker-web-app",
  "repository": "github.com/jmartinezhern/pointing-poker",
  "version": "0.0.1",
  "description": "Web Application for pointingpoker.app",
  "author": "Jorge Martinez Hernandez",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://pointingpoker.app",
  "scripts": {
    "clean": "rm -rf ./dist ./.cache ./node_modules ./src/graphql/generated-types.ts",
    "codegen": "graphql-codegen -r dotenv/config --config codegen.yml",
    "commit": "git-cz",
    "tsc:check": "tsc --noEmit -p .",
    "eslint:check": "eslint 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "test": "jest --runInBand",
    "test:coverage": "npm run test -- --coverage",
    "build": "parcel build --no-cache src/index.html",
    "test:watch": "jest --watch",
    "storybook:start": "start-storybook",
    "storybook:build": "build-storybook -c .storybook -o dist-storybook"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "moduleNameMapper": {
      "^~(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": {
    "production": [
      ">1% in US",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.3.15",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "graphql": "15.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-charts": "3.0.15",
    "react-router-dom": "5.2.0",
    "subscriptions-transport-ws": "0.9.18",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@graphql-codegen/typescript-resolvers": "1.19.0",
    "@storybook/addon-actions": "6.2.8",
    "@storybook/addon-links": "6.2.8",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addons": "6.2.8",
    "@storybook/cli": "6.2.8",
    "@storybook/react": "6.2.8",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.39",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.1.7",
    "@types/uuid": "8.3.0",
    "@types/ws": "7.4.1",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-loader": "8.2.2",
    "babel-types": "6.26.0",
    "codecov": "3.8.1",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.3",
    "less": "4.1.1",
    "parcel-bundler": "1.12.5",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-loader": "8.1.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
